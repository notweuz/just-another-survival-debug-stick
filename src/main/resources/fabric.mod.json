{
  "schemaVersion": 1,
  "id": "survivaldebugstick",
  "version": "${version}",
  "name": "Survival Debug Stick",
  "description": "Allow players to use debug stick in survival mode.\n\nHighly inspired by Just_S' survival debug stick mod, but more lightweight.",
  "authors": [
    "notweuz"
  ],
  "contact": {
    "sources": "https://github.com/notweuz/survival-debug-stick",
    "issues": "https://github.com/notweuz/survival-debug-stick/issues"
  },
  "license": "MIT",
  "icon": "assets/survivaldebugstick/icon.png",
  "environment": "*",
  "entrypoints": {
    "main": [
      "ru.ntwz.survivaldebugstick.SurvivalDebugStick"
    ]
  },
  "mixins": [
    "survivaldebugstick.mixins.json"
  ],
  "depends": {
    "fabricloader": ">=${loader_version}",
    "fabric": "*",
    "minecraft": "${minecraft_version}"
  }
}
